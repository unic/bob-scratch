<?xml version="1.0" encoding="utf-8"?>
<Configuration>
  <WebsiteCodeName>SitecoreWebsite</WebsiteCodeName>
  <WebFolderName>Web</WebFolderName>
  <GlobalWebPath>c:\IISWebsites</GlobalWebPath>
  <SitecoreVersion>9.0.171002.123</SitecoreVersion>
  <!-- leave empty to use default App_Data -->
  <DataFolderPath></DataFolderPath>
  <HostsFileComment>generated by Bob Open Build and his Can Do Crew</HostsFileComment>
  <DatabaseServer>localhost</DatabaseServer>
  <DatabaseDbPrefix>SitecoreWebsiteDB</DatabaseDbPrefix>
  <DatabaseAdminUser>sa</DatabaseAdminUser>
  <DatabaseAdminPwd></DatabaseAdminPwd>
