  </XConnectCollectionServiceUrl>
  <XConnectClientCertificateName>sitecorewebsite.xconnect_client</XConnectClientCertificateName>
  <XConnectWebsiteCodename>sitecorewebsite.xconnect</XConnectWebsiteCodename>
  <XConnectWebFoldername></XConnectWebFoldername>
  <IISBindings>
    <Binding IP="127.0.0.10">http://sitecorewebsite.local</Binding>
    <Binding IP="127.0.0.20">https://sitecorewebsite.local</Binding>
  </IISBindings>
